{
  "name": "@xizher/esri",
  "version": "1.0.13",
  "description": "基于ArcGIS API for JavaScirpt的业务功能型二次开发地图API",
  "main": "dist/index.js",
  "scripts": {
    "copy": "npx ncp ./node_modules/@arcgis/core/assets ./__tests__/src/assets",
    "uml": "npx npx tplant --input src/**/*.ts --output tsuml.svg",
    "test": "vite",
    "build": "npx tsc -w",
    "update-xizher": "npm install --save @xizher/js-ext@latest @xizher/js-utils@latest @xizher/observer@latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xizher/xizher-esri.git"
  },
  "keywords": [
    "ArcGIS API for JavaScirpt 4.18+",
    "@arcgis/core",
    "TypeScript"
  ],
  "author": "wuxizhe",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xizher/xizher-esri/issues"
  },
  "homepage": "https://github.com/xizher/xizher-esri#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "@vitejs/plugin-vue": "^1.2.1",
    "@vue/compiler-sfc": "^3.0.11",
    "eslint": "^7.23.0",
    "ncp": "^2.0.0",
    "tplant": "^2.3.6",
    "typescript": "^4.2.3",
    "vite": "^2.3.3",
    "vue": "^3.0.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.0.0-beta.1",
    "vue": "^2.0.0 || >=3.0.0-rc.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "@arcgis/core": "^4.19.3",
    "@xizher/core": "^1.4.1",
    "vue-demi": "^0.9.1"
  }
